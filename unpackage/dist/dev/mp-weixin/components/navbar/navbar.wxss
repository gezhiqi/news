.navbar .navbar-fixed {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
  width: 100%;
  width: 100%;
  background-color: #f07373;
}
.navbar .navbar-fixed .navbar-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
  height: 45px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.navbar .navbar-fixed .navbar-content .navbar-search {
  padding: 0 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 32px;
  -webkit-border-radius: 30px;
          border-radius: 30px;
  background-color: #fff;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search_icon {
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search_text {
  font-size: 12px;
  color: #999;
}

